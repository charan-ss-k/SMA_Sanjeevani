# Backend API Configuration
# Set to your Python backend server IP address (e.g., http://192.168.1.100:8000)
REACT_APP_API_BASE_URL=http://192.168.1.100:8000
REACT_APP_API_TIMEOUT=30000

# Environment
REACT_APP_ENV=development

# Feature Flags
REACT_APP_ENABLE_AI_STREAMING=true
REACT_APP_ENABLE_TTS=true
REACT_APP_ENABLE_OFFLINE_MODE=false

# Logging
REACT_APP_DEBUG_LOGS=true
